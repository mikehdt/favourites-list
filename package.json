{
  "name": "favourites-list",
  "version": "1.0.0",
  "description": "Favourites list!",
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "body-parser": "^1.15.0",
    "chimp": "^0.30.0",
    "concurrently": "^2.0.0",
    "express": "^4.13.4"
  },
  "scripts": {
    "test:features": "concurrently --kill-others \"node ./api/server.js\" \"chimp --path=./features --browser=phantomjs\"",
    "test:features:watch": "concurrently \"node ./api/server.js\" \"chimp --watch --path=./features --browser=phantomjs\"",
    "api": "node ./api/server.js"
  },
